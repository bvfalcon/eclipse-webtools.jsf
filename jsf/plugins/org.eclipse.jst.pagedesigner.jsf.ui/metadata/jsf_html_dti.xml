	<entity id="column" type="tag">
		<trait id="dt-info">
			<value xsi:type="dti:DTInfo">
				<tag-convert-info classname="org.eclipse.jst.pagedesigner.jsf.ui.converter.operations.jsf.ColumnOperation"/>
				<tag-decorate-info id="vpd-decorate-design"
